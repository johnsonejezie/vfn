{
  "name": "vfn",
  "version": "1.0.0",
  "description": "Vote For Nigeria",
  "main": "index.js",
  "author": "Johnson Ejezie",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "npm run apidoc && node dist/index.js",
    "dev": "npm run apidoc && nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "apidoc": "apidoc -i src/routes/ -o src/public/apidoc",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "apidoc": "^0.17.7",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "logger": "^0.0.1",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "sendbird": "^3.0.92"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "nodemon": "^1.17.3"
  }
}
